{
  "name": "react-node-webpack-boilerplate",
  "version": "1.2.14",
  "description": "React/Nodejs + Webpack",
  "main": "public/main.js",
  "entry": "src/client/",
  "scripts": {
    "build": "rm -rf public && npm i --legacy-peer-deps --no-audit && npm run build-css && npm run client:build && npm run image-copy && echo \"Successfully build webpwa-\"$npm_package_version ",
    "dev": "npm run build-css && webpack-dev-server --config ./config/webpack.dev.config.js",
    "start-production": "pm2 start config/server/server.js -f --name 'WebPWA' -l --log WebPWA.log",
    "stop-production": "sh script.sh delete-pm2-process",
    "restart-production": "sh script.sh delete-pm2-process && npm run start-production",
    "version_ping": "echo $npm_package_version",
    "server:build": "webpack --progress --config ./config/webpack.server.config.js --env package_version=$npm_package_version",
    "client:build": "webpack --progress --config ./config/webpack.client.config.js",
    "analyze": "webpack --progress --config ./config/webpack.analyzer.config.js",
    "remove-public": "rm -rf public",
    "config-env": "node -e 'require(\"./config\").config()'",
    "build-css": "sass src/client/scss:src/client/css",
    "image-copy": "mkdir ./public/client/images && cp -R src/client/images/* ./public/client/images/"
  },
  "license": "MIT",
  "devDependencies": {
